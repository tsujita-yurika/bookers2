 <div class = "container px-5 px-sm-0">
    <div class = "row">
     <%= render 'books/tenpure-to', user: @user %>

      <div class = "col-md-8 offset-md-1">
        　<h2>Users</h2>
      　 <table class="table table-hover table-inverse">
       　  <thead>
       　    <tr>
        　     <th>Image</th>
        　     <th>name</th>
        　     <th colspan = "3"></th>
        　   </tr>
       　  </thear>
      　   <tbody>
            <% @books.each do |book| %>
              <tr>
                <td><%= image_tag book.user.get_profile_image(80,80) %></td>
                <td><%= book.user.name %></td>
                <td><%= link_to "Show",user_path(book.user) %></td>
              </tr>
            <% end %>
       　  </tbody>
     　   </table>
      </div>

    </div>
  </div>
